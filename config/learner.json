{
	"seed": 0,
	"input": {
		"max_sample_count": 10000,
		"min_sample_length": 5,
		"max_sample_length": 50,
		"training_fraction": 0.7,
		"standard_count": 1000
	},
	"model": {
		"dimension_count": 1,
		"layer_count": 1,
		"unit_count": 200,
		"cell": {
			"name": "LSTM",
			"options": {
				"cell_clip": 1.0,
				"forget_bias": 1.0,
				"use_peepholes": true
			}
		},
		"initializer": {
			"name": "uniform",
			"options": {
				"minval": -0.01,
				"maxval": 0.01
			}
		},
		"dropout": {
			"options": {
				"input_keep_prob": 1.0,
				"output_keep_prob": 1.0
			}
		}
	},
	"teacher": {
		"gradient_clip": 1.0,
		"optimizer": {
			"name": "Adam",
			"options": {
				"learning_rate": 1e-4
			}
		},
		"future_length": 4
	},
	"manager": {
		"backup_period": 10000,
		"validation_period": 10000
	},
	"output": {
		"baseline": true
	}
